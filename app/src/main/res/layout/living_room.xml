<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/lightBlue"
    android:orientation="vertical">


<androidx.cardview.widget.CardView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:cardElevation="8dp"
    android:layout_marginTop="20dp">
    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="120dp"
        android:layout_marginTop="10dp">

        <LinearLayout
            android:id="@+id/llSofa1"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp">

            <ImageView
                android:id="@+id/ms_image"
                android:layout_width="65dp"
                android:layout_height="65dp"

                android:src="@drawable/sofa"/>

            <TextView
                android:id="@+id/ms_title1"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:text="Sofas"
                android:textSize="18dp"
                android:gravity="center" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llSofa2"

            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@id/llSofa1"
            android:layout_marginLeft="25dp"
            android:layout_toRightOf="@id/llSofa1">

            <ImageView
                android:id="@+id/ms_image1"
                android:layout_width="65dp"
                android:layout_height="65dp"

                android:src="@drawable/ic_bookshelf_1"/>

            <TextView
                android:id="@+id/ms_title11"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:text="Bookshelves"
                android:textSize="18dp"
                android:gravity="center" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llSofa3"

            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@id/llSofa2"
            android:layout_marginLeft="25dp"
            android:layout_toRightOf="@id/llSofa2">

            <ImageView
                android:id="@+id/ms_image2"
                android:layout_width="65dp"
                android:layout_height="65dp"

                android:src="@drawable/ic_table_7"/>

            <TextView
                android:id="@+id/ms_title12"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:text="Tables"
                android:textSize="18dp"
                android:gravity="center"
                />

        </LinearLayout>


        <LinearLayout
            android:id="@+id/llSofa4"

            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@id/llSofa3"
            android:layout_marginLeft="25dp"
            android:layout_toRightOf="@id/llSofa3">

            <ImageView
                android:id="@+id/ms_image3"
                android:layout_width="65dp"
                android:layout_height="65dp"

                android:src="@drawable/ic_lights"/>

            <TextView
                android:id="@+id/ms_title13"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="Lights"
                android:textSize="18dp"
                android:gravity="center"
                />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llSofa5"

            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@id/llSofa4"
            android:layout_marginLeft="25dp"
            android:layout_toRightOf="@id/llSofa4">

            <ImageView
                android:id="@+id/ms_image4"
                android:layout_width="65dp"
                android:layout_height="65dp"

                android:src="@drawable/ic_armchair_1"/>

            <TextView
                android:id="@+id/ms_title14"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:text="Armchairs"
                android:textSize="18dp"
                android:gravity="center"
                />

        </LinearLayout>
    </RelativeLayout>
    </ScrollView>
</androidx.cardview.widget.CardView>


    <RelativeLayout
